{
    "name": "laravel/passport",
    "description": "Laravel Passport provides OAuth2 server support to Laravel.",
    "keywords": ["laravel", "passport", "oauth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel/passport/issues",
        "source": "https://github.com/laravel/passport"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "firebase/php-jwt": "^3.0|^4.0|^5.0",
        "guzzlehttp/guzzle": "^6.0",
        "illuminate/auth": "^6.18.30|^7.22.3",
        "illuminate/console": "^6.18.30|^7.22.3",
        "illuminate/container": "^6.18.30|^7.22.3",
        "illuminate/contracts": "^6.18.30|^7.22.3",
        "illuminate/cookie": "^6.18.30|^7.22.3",
        "illuminate/database": "^6.18.30|^7.22.3",
        "illuminate/encryption": "^6.18.30|^7.22.3",
        "illuminate/http": "^6.18.30|^7.22.3",
        "illuminate/support": "^6.18.30|^7.22.3",
        "league/oauth2-server": "dev-device-flow-grant#d3f0a791e4f11b2bbdc94b40d2e47b55d9bd01b8",
        "phpseclib/phpseclib": "^2.0",
        "symfony/psr-http-message-bridge": "^1.0",
        "zendframework/zend-diactoros": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Passport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Passport\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Passport\\PassportServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lucadegasperi/oauth2-server"
        }
    ]
}
