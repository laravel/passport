{
    "name": "laravel/passport",
    "description": "Laravel Passport provides OAuth2 server support to Laravel. This is a fork that implement support to Uuid in all models for Oauth by HTML5 FÃ¡cil community.",
    "keywords": ["laravel", "passport", "oauth", "uuid"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Luis Antonio Gomez",
            "email": "jimmylagp@gmail.com"
        },
        {
            "name": "Misraim Mendoza",
            "email": "michmendar@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "alsofronie/eloquent-uuid",
                "version": "dev-master",
                "source": {
                    "url": "git@github.com:html5facil/eloquent-uuid.git",
                    "type": "git",
                    "reference": "origin/master"
                },
                "require": {
                    "webpatser/laravel-uuid": "2.*"
                },
                "require-dev": {
                    "laravel/framework": "^5.1.0",
                    "phpunit/phpunit": "~4.0"
                },
                "autoload": {
                    "psr-4": {
                        "Alsofronie\\Uuid\\": "src/"
                    }
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "alsofronie/eloquent-uuid": "dev-master",
        "firebase/php-jwt": "~3.0|~4.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/auth": "~5.3",
        "illuminate/console": "~5.3",
        "illuminate/container": "~5.3",
        "illuminate/contracts": "~5.3",
        "illuminate/database": "~5.3",
        "illuminate/encryption": "~5.3",
        "illuminate/http": "~5.3",
        "illuminate/support": "~5.3",
        "league/oauth2-server": "~5.0",
        "symfony/psr-http-message-bridge": "^0.3.0",
        "zendframework/zend-diactoros": "~1.0",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Passport\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
